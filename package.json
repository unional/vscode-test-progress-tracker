{
  "name": "vscode-test-progress-tracker",
  "version": "0.0.1",
  "description": "Show Test Progress",
  "homepage": "https://github.com/unional/vscode-test-progress-tracker#readme",
  "bugs": {
    "url": "https://github.com/unional/vscode-test-progress-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unional/vscode-test-progress-tracker.git"
  },
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -p tsconfig.json src/**/*.ts",
    "test": "npm run compile && cross-env CODE_TESTS_WORKSPACE=testProject && node ./node_modules/vscode/bin/test",
    "vscode:prepublish": "npm run compile",
    "watch": "node scripts/watch.js"
  },
  "dependencies": {
    "@unional/test-progress-tracker": "^1.3.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@unional/jest-progress-reporter": "^1.0.0",
    "assertron": "^5.2.0",
    "cross-env": "^5.2.0",
    "delay": "^4.1.0",
    "jest": "^23.6.0",
    "jest-audio-reporter": "^2.2.0",
    "jest-environment-node": "^23.4.0",
    "jest-junit": "^5.2.0",
    "jest-watch-suspend": "^1.1.0",
    "jest-watch-toggle-config": "^1.0.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-unional": "^0.10.0",
    "typescript": "^3.1.3",
    "vscode": "^1.1.21"
  },
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:progressTrackerTreeView"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "progressTrackerTreeView",
          "name": "Test Progress"
        }
      ]
    }
  },
  "displayName": "Test Progress Tracker",
  "publisher": "unional"
}
