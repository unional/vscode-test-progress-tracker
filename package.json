{
  "name": "vscode-test-progress-tracker",
  "version": "0.2.1",
  "description": "Show Test Progress",
  "homepage": "https://github.com/unional/vscode-test-progress-tracker#readme",
  "bugs": {
    "url": "https://github.com/unional/vscode-test-progress-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unional/vscode-test-progress-tracker.git"
  },
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -p tsconfig.json src/**/*.ts",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "vscode:prepublish": "npm run compile",
    "watch": "jest --watch"
  },
  "dependencies": {
    "progress-str": "^3.4.0",
    "test-progress-tracker": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "assertron": "^6.0.0",
    "cross-env": "^5.2.0",
    "delay": "^4.1.0",
    "jest": "^23.6.0",
    "jest-audio-reporter": "^2.2.1",
    "jest-environment-node": "^23.4.0",
    "jest-junit": "^6.0.1",
    "jest-progress-tracker": "^2.0.1",
    "jest-watch-suspend": "^1.1.1",
    "jest-watch-toggle-config": "^1.0.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-unional": "^0.10.0",
    "typescript": "^3.2.2",
    "vscode": "^1.1.26"
  },
  "engines": {
    "vscode": "^1.25.0"
  },
  "activationEvents": [
    "onView:progressTrackerTreeView"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "progressTrackerTreeView",
          "name": "Test Progress"
        }
      ]
    }
  },
  "displayName": "Test Progress Tracker",
  "publisher": "unional"
}
